#include <bits/stdc++.h>

using namespace std;

string toStr(int x){
	stringstream s;
	s << x;
	return s.str();
}

int toInt(string x){
	stringstream s;
	s << x;
	int r;
	s >> r;
	return r;
}

int main(){
	vector<int> impares(500000000002);
	for(int i=0; i<impares.size(); i++){
		impares[i] = 2*i+1;
	}
	cout<<impares[33000]<<endl;
	return 0;
}